{
	"name": "abmat/craft-checkit",
	"description": "Effortlessly mark section entries to streamline collaboration and content organization.",
	"type": "craft-plugin",
	"license": "proprietary",
	"authors": [
	  {
		"name": "ABM Feregyhazy & Simon GmbH",
		"homepage": "https://www.abm.at"
	  }
	],
	"require": {
		"craftcms/cms": "^5.0.0-beta"
	},
	"support": {
		"issues": "https://github.com/abm-werbeagentur/craft-checkit/issues",
    	"source": "https://github.com/abm-werbeagentur/craft-checkit"
	},
	"autoload": {
	  "psr-4": {
		"abmat\\checkit\\": "src/"
	  }
	},
	"extra": {
	  "name": "Check it",
	  "handle": "abm-checkit",
	  "class": "abmat\\checkit\\CheckIt"
	},
	"config": {
		"allow-plugins": {
			"yiisoft/yii2-composer": true,
			"craftcms/plugin-installer": true
		}
	}
}
